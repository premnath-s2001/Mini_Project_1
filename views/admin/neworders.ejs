    <body style="background: linear-gradient(to top, #353535ba, #000000a6), url(/images/orders1.jpg) no-repeat top center;background-repeat:no-repeat;background-size:cover;background-attachment: fixed;">
        
        <main>
            
            <!--MDB Tables-->
            <div class="container mt-4">
    
                
                <div class="card mb-4">
                    <div class="card-body">
                        <!-- Grid row -->
                        <div class="row">
                            <!-- Grid column -->
                            <div class="col-md-12">
                                <h2 class="pt-3 pb-4 text-center font-bold font-up deep-purple-text">Recent orders by the customers</h2>
                                
                            </div>
                            <!-- Grid column -->
                        </div>
                        <!-- Grid row -->
                        <!--Table-->
                        <table class="table table-striped">
                            <!--Table head-->
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Name</th>
                                    <th>Mobile</th>
                                    <th>Products</th>
                                    <th>Quantity</th>
                                    <th>Price</th>
                                    <th>Pincode</th>                                    
                                    <th>Address</th>
                                </tr>
                            </thead>
                            <!--Table head-->
                            <!--Table body-->
                            <tbody>
                                <% for (var i =1; i <=10;  i++ ) { %>
                                <tr>
                                    <th scope="row"><%=i%></th>
                                    <td>Ravi</td>
                                    <td>6383344764</td>
                                    <td>Apple,Orange</td>
                                    <td>6</td>
                                    <td>â‚¹240</td>
                                    <td>638751</td>
                                    <td>27,Kinathankadu,Uthukuli.</td>
                                </tr>
                                <% } %>
                            </tbody>
                            <!--Table body-->
                        </table>
                        <!--Table-->
                    </div>
                </div>
                
            </div>
            <!--MDB Tables-->
          
        </main>
      

</body>
</html>